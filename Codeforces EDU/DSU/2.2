int getParent(int x, int *parent)
{
	if (parent[x]  == x || parent[x] == -1)
		return parent[x];
	return parent[x] = getParent(parent[x], parent);
}
int join(int a, int b, int *parent)
{
	int parb = getParent(b, parent);
	parent[a] = parb;
}
